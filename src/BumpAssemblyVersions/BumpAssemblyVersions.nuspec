<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <!-- Required elements-->
        <id>$id$</id>
        <version>$version$</version>
        <title>$title$</title>
        <authors>$author$</authors>
        <owners>$owners$</owners>
        <summary>$summary$</summary>
        <description>$description$</description>
        <developmentDependency>$developmentDependency$</developmentDependency>
        <license type="file">$licenseFile$</license>
        <projectUrl>$projectUrl$</projectUrl>
        <requireLicenseAcceptance>$requireLicenseAcceptance$</requireLicenseAcceptance>
        <tags>$packageTags$</tags>
    </metadata>
    <files>
        <file src="$licenseFile$" target="" />
        <file src="bin\$config$\$targetFramework$\$id$.dll" target="tasks" />
        <file src="bin\$config$\$targetFramework$\$id$.xml" target="tasks" />
        <file src="bin\$config$\$targetFramework$\*" target="tasks\$targetFramework$" />
        <!-- ReSharper disable MarkupAttributeTypo -->
        <file src="obj\$config$\netstandard2.0\build\*.*" target="build" />
    </files>
</package>