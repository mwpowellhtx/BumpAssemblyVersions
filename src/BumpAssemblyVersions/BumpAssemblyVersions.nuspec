<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <!-- Required elements-->
        <id>$id$</id>
        <version>$version$</version>
        <title>$title$</title>
        <authors>$author$</authors>
        <owners>$owners$</owners>
        <summary>$summary$</summary>
        <description>$description$</description>
        <developmentDependency>$developmentDependency$</developmentDependency>
        <license type="file">$licenseFile$</license>
        <licenseUrl>https://aka.ms/deprecateLicenseUrl</licenseUrl>
        <projectUrl>$projectUrl$</projectUrl>
        <requireLicenseAcceptance>$requireLicenseAcceptance$</requireLicenseAcceptance>
        <tags>$packageTags$</tags>
        <repository type="$repoType$" branch="$repoBranch$" url="$projectUrl$" commit="$repoCommit$" />
    </metadata>
    <files>
        <file src="$licenseFile$" target="" />
        <file src="bin\$config$\$targetFramework$\*.dll" target="tasks\$targetFramework$" />
        <file src="bin\$config$\$targetFramework$\*.json" target="tasks\$targetFramework$" />
        <file src="bin\$config$\$targetFramework$\*.pdb" target="tasks\$targetFramework$" />
        <file src="bin\$config$\$targetFramework$\*.xml" target="tasks\$targetFramework$" />
        <file src="bin\$config$\$targetFramework$\build\*.*" target="build" />
        <file src="bin\$config$\$targetFramework$\buildMultiTargeting\*.*" target="buildMultiTargeting" />
        <file src="bin\$config$\$targetFramework$\buildTransitive\*.*" target="buildTransitive" />
    </files>
</package>